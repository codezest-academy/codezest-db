{
  "name": "@codezest-academy/codezest-db",
  "version": "1.1.0",
  "description": "Single source of truth for CodeZest database schema & types",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "prisma"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./prisma": "./prisma/schema.prisma",
    "./mongo": "./dist/mongo/index.js"
  },
  "scripts": {
    "build": "tsc && prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "release": "npm version patch && npm publish --access public",
    "clean": "rm -rf dist node_modules",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "prisma",
    "database",
    "schema",
    "codezest",
    "learning-platform",
    "typescript"
  ],
  "author": "CodeZest Academy",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "prisma": "^5.20.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "winston": "^3.18.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codezest-academy/codezest-db.git"
  },
  "bugs": {
    "url": "https://github.com/codezest-academy/codezest-db/issues"
  },
  "homepage": "https://github.com/codezest-academy/codezest-db#readme"
}
